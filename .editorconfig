; http://editorconfig.org/
; https://github.com/editorconfig/editorconfig-core-go/blob/master/.editorconfig

root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.md]
indent_style = tab
indent_size = 4

[*.go]
indent_style = tab
indent_size = 4
